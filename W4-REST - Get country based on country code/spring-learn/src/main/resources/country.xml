<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

    <bean id="countryIndia" class="com.cognizant.spring_learn.model.Country">
        <constructor-arg value="IN"/>
        <constructor-arg value="India"/>
    </bean>

    <bean id="countryUSA" class="com.cognizant.spring_learn.model.Country">
        <constructor-arg value="US"/>
        <constructor-arg value="United States"/>
    </bean>
    
</beans>
